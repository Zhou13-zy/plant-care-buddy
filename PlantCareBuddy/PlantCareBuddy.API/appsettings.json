{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Cors": {
        "AllowedOrigins": [ "http://localhost:3000" ]
    },
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost;Database=PlantCareBuddyDb;Trusted_Connection=True;TrustServerCertificate=True;"
    },
    "PhotoStorage": {
        "StoragePath": "wwwroot/images",
        "AllowedExtensions": [ ".jpg", ".jpeg", ".png" ],
        "MaxFileSize": 5242880,
        "BaseUrl": "/images"
    }
}
